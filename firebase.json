{
  "hosting": {
    "source": ".",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "frameworksBackend": {
      "region": "us-central1"
    },
    "redirects": [
      {
        "source": "/:path*",
        "destination": "https://innovativeenterprises.tech/:path*",
        "type": 301,
        "protocol": "http"
      }
    ]
  }
}
